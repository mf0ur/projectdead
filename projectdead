local function ZMOSEK_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.maaa.Visible = not script.Parent.Parent.maaa.Visible
	end)
	
end
coroutine.wrap(ZMOSEK_fake_script)()
local function DQVS_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.ererer.Visible = false
		script.Parent.Parent.tyty.Visible = false
		script.Parent.Parent.look.Visible = false
		script.Parent.Parent.ghgh.Visible = false
	end)
	
